{
  "name": "bluegg-module-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for making NPM ready modules using ES2015 syntax",
  "main": "dist/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel --plugins add-module-exports src/index.js > dist/index.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bluegg/bluegg-module-boilerplate.git"
  },
  "author": "bluegg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bluegg/bluegg-module-boilerplate/issues"
  },
  "homepage": "https://github.com/Bluegg/bluegg-module-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.9.0"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "parsleyjs": "^2.4.4"
  }
}
